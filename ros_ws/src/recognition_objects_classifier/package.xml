<?xml version="1.0"?>
<package format="2">
    <name>recognition_objects_classifier</name>
    <version>0.0.1</version>
    <description>
        Takes objects (rects, circles, segments) and classifies them as outside the map or as inside the map.
    </description>
    <license>GPLv3</license>
    <author email="utcoupe@assos.utc.fr">milesial</author>
    <maintainer email="utcoupe@assos.utc.fr">UTCoupe</maintainer>
    <buildtool_depend>catkin</buildtool_depend>
    <depend>rospy</depend>
    <depend>message_generation</depend>
    <depend>tf2</depend>
    <depend>tf2_ros</depend>
    <exec_depend>message_runtime</exec_depend>
    <export>
    </export>
</package>
